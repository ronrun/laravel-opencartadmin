{
    "name": "ronrun/ocadmin",
    "description": "Integrate OpenCart Admin with Laravel backend.",
    "keywords": ["laravel", "opencart", "admin"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ronrun\\Ocadmin\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ronrun/ocadmin"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Ronrun\\Ocadmin\\OcadminServiceProvider"
            ]
        }
    }
}
